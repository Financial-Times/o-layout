<div class="o-layout" data-o-component="o-syntax-highlight">
{{> shared/header}}

	<div class="o-layout__area--sidebar">
		<ol class="o-layout__navigation">
			{{#children}}
				{{#current}}
					{{#navigation}}
						<li>
							<a href="{{href}}">{{name}}</a>
						</li>
					{{/navigation}}
				{{/current}}
			{{/children}}
		</ol>
	</div>

	<div class="o-layout__area--main">
		{{> content/quickstart-tutorial}}
	</div>

{{> shared/footer}}
</div>
